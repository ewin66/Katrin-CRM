<?xml version="1.0"?>
<ArrayOfFormatCondition xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <FormatCondition>
    <Name>High Priority</Name>
    <ConditionName>HighPriority</ConditionName>
    <Backcolor />
    <BackcolorArgb>0</BackcolorArgb>
    <Forecolor />
    <ForecolorArgb>0</ForecolorArgb>
    <FontName>微软雅黑</FontName>
    <Size>8.5</Size>
    <Bold>true</Bold>
    <Unit>false</Unit>
    <Italic>false</Italic>
    <Underline>false</Underline>
    <Conditions>[PriorityCode] = 1 Or [PriorityCode] = 2</Conditions>
    <Active>true</Active>
    <UseFont>true</UseFont>
    <UseForeColor>false</UseForeColor>
    <UseBackColor>false</UseBackColor>
  </FormatCondition>
  <FormatCondition>
    <Name>Dead</Name>
    <ConditionName>Dead</ConditionName>
    <Backcolor />
    <BackcolorArgb>0</BackcolorArgb>
    <Forecolor />
    <ForecolorArgb>-65536</ForecolorArgb>
    <FontName>微软雅黑</FontName>
    <Size>8.5</Size>
    <Bold>false</Bold>
    <Unit>false</Unit>
    <Italic>false</Italic>
    <Underline>false</Underline>
    <Conditions>[StatusCode] = 6</Conditions>
    <Active>true</Active>
    <UseFont>false</UseFont>
    <UseForeColor>true</UseForeColor>
    <UseBackColor>false</UseBackColor>
  </FormatCondition>
  <FormatCondition>
    <Name>Active Leads Didn't Update In Two Weeks</Name>
    <ConditionName>ActiveLeadsDidn'tUpdateInTwoWeeks</ConditionName>
    <Backcolor />
    <BackcolorArgb>0</BackcolorArgb>
    <Forecolor />
    <ForecolorArgb>-16777024</ForecolorArgb>
    <FontName>微软雅黑</FontName>
    <Size>8.5</Size>
    <Bold>false</Bold>
    <Unit>false</Unit>
    <Italic>false</Italic>
    <Underline>false</Underline>
    <Conditions>Not [StatusCode] In (4, 6) And IsPriorDays([ModifiedOn],14)</Conditions>
    <Active>true</Active>
    <UseFont>false</UseFont>
    <UseForeColor>true</UseForeColor>
    <UseBackColor>false</UseBackColor>
  </FormatCondition>
</ArrayOfFormatCondition>