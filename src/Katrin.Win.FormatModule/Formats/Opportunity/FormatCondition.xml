<?xml version="1.0"?>
<ArrayOfFormatCondition xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <FormatCondition>
    <Name>High Priority</Name>
    <ConditionName>HighPriority</ConditionName>
    <Backcolor />
    <BackcolorArgb>0</BackcolorArgb>
    <Forecolor />
    <ForecolorArgb>0</ForecolorArgb>
    <FontName>微软雅黑</FontName>
    <Size>8.5</Size>
    <Bold>true</Bold>
    <Unit>false</Unit>
    <Italic>false</Italic>
    <Underline>false</Underline>
    <Conditions>[PriorityCode] = 1 Or [PriorityCode] = 2</Conditions>
    <Active>true</Active>
    <UseFont>true</UseFont>
    <UseForeColor>false</UseForeColor>
    <UseBackColor>false</UseBackColor>
  </FormatCondition>
  <FormatCondition>
    <Name>Great Estimated Value</Name>
    <ConditionName>GreatEstimatedValue</ConditionName>
    <Backcolor />
    <BackcolorArgb>-32</BackcolorArgb>
    <Forecolor />
    <ForecolorArgb>0</ForecolorArgb>
    <FontName>微软雅黑</FontName>
    <Size>8.5</Size>
    <Bold>false</Bold>
    <Unit>false</Unit>
    <Italic>false</Italic>
    <Underline>false</Underline>
    <Conditions>[EstimatedValue] &gt; 10000.0m</Conditions>
    <Active>true</Active>
    <UseFont>false</UseFont>
    <UseForeColor>false</UseForeColor>
    <UseBackColor>true</UseBackColor>
  </FormatCondition>
  <FormatCondition>
    <Name>Lost Opportunities</Name>
    <ConditionName>LostOpportunities</ConditionName>
    <Backcolor />
    <BackcolorArgb>0</BackcolorArgb>
    <Forecolor />
    <ForecolorArgb>-65536</ForecolorArgb>
    <FontName>微软雅黑</FontName>
    <Size>8.5</Size>
    <Bold>false</Bold>
    <Unit>false</Unit>
    <Italic>false</Italic>
    <Underline>false</Underline>
    <Conditions>[StatusCode] = 4</Conditions>
    <Active>true</Active>
    <UseFont>false</UseFont>
    <UseForeColor>true</UseForeColor>
    <UseBackColor>false</UseBackColor>
  </FormatCondition>
  <FormatCondition>
    <Name>Active Opportunies Didn't Update In Amonth</Name>
    <ConditionName>ActiveOpportuniesDidn'tUpdateInAmonth</ConditionName>
    <Backcolor />
    <BackcolorArgb>0</BackcolorArgb>
    <Forecolor />
    <ForecolorArgb>-16777024</ForecolorArgb>
    <FontName>微软雅黑</FontName>
    <Size>8.5</Size>
    <Bold>false</Bold>
    <Unit>false</Unit>
    <Italic>false</Italic>
    <Underline>false</Underline>
    <Conditions>[StatusCode] = 1 And IsPriorDays([ModifiedOn],30)</Conditions>
    <Active>true</Active>
    <UseFont>false</UseFont>
    <UseForeColor>true</UseForeColor>
    <UseBackColor>false</UseBackColor>
  </FormatCondition>
</ArrayOfFormatCondition>